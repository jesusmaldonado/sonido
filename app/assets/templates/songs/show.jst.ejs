<ul class="song">



    <li> Title:  <%= song.escape("title") %> </li>


    <li> Created At: <%= song.escape("created_at") %> </li>


    <li> Username:  <%= song.user().escape("username") %> </li>


    <li> Record Title: <%= song.recording().escape("title") %> </li>

    <button class="playSong">
      >
    </button>
    <div class="renderer">
    </div>
    <audio src="<%= song.escape('audio_url') %>" id="Audio">
    </audio>

    <button class="destroySong button" data-id="<%= song.escape('id') %>">
      Destroy this song!
    </button>

    <button class="songLikeStatus button" data-liked="<%= song.escape('liked') %>" data-id="<%= song.id %>">
      Like this song!
    </button>
</ul>
